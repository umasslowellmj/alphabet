<p id="notice"><%= notice %></p>

<h1>Eng Word Translations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eng_word_translations.each do |eng_word_translation| %>
      <tr>
        <td><%= link_to 'Show', eng_word_translation %></td>
        <td><%= link_to 'Edit', edit_eng_word_translation_path(eng_word_translation) %></td>
        <td><%= link_to 'Destroy', eng_word_translation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

